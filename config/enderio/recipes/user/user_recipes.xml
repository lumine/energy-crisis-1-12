<?xml version="1.0" encoding="UTF-8"?>
<enderio:recipes xmlns:enderio="http://enderio.com/recipes" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://enderio.com/recipes recipes.xsd ">
  <recipe name="Sagmill: oreLead Ore" required="false">
    <sagmilling energy="3600">
      <input name="oreLead" />
      <output name="dustLead" amount="2" />
      <output name="dustSilver" chance="1.0" required="false" />
      <output name="oredict:cobblestone" chance="0.15" />
    </sagmilling>
  </recipe>
  <recipe name="Sagmill: oreAluminum Ore" required="false">
    <sagmilling energy="3600">
      <input name="oreAluminum" />
      <output name="dustAluminum" amount="2" />
      <output name="dustTitanium" chance="1.0" required="false" />
      <output name="oredict:cobblestone" chance="0.15" />
    </sagmilling>
  </recipe>
</enderio:recipes>
